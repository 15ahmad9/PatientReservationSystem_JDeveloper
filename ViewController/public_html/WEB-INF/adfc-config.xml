<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="Doctor">
    <page>/Doctor.jsf</page>
  </view>
  <view id="Patient">
    <page>/Patient.jsf</page>
  </view>
  <view id="Payment">
    <page>/Payment.jsf</page>
  </view>
  <view id="Appointment">
    <page>/Appointment.jsf</page>
  </view>
  <view id="Home">
    <page>/Home.jsf</page>
  </view>
  <view id="SearchPatient">
    <page>/SearchPatient.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>doctor</from-outcome>
      <to-activity-id>Doctor</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__4">
      <from-outcome>patient</from-outcome>
      <to-activity-id>Patient</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>payment</from-outcome>
      <to-activity-id>Payment</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__6">
      <from-outcome>appointment</from-outcome>
      <to-activity-id>Appointment</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__8">
      <from-outcome>home</from-outcome>
      <to-activity-id>Home</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__10">
      <from-outcome>*searchPatient</from-outcome>
      <to-activity-id>SearchPatient</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
